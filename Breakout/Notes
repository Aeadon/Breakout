If(hits player){
	y-direction must equal positive integer
}

c^2 = a^2 + b^2
hyp = sqrt(x-dir^2 + y-dir^2);
angle = tangent(y-magnitude / x-magnitude)
new_angle = 180-angle;	


[1|2|3|4|5]
1 = x positive? x*= -1.4, else x*=1.4, y*= -1
2 = x positive? x*= -1.2 else x*=1.2, y*= -1
3 = x same, y *= -1
4 = x positive? x *= 1.2 else -1.2, y*= -1
5 = x positive? x *= -1.4 else -1.2, y*= -1